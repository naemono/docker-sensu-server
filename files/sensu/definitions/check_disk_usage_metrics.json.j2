{
  "checks": {
    "disk_usage_metrics": {
      "type": "metric",
      "command": "/opt/sensu/embedded/bin/ruby {{sensu_config_path}}/plugins/metrics-disk-usage.rb",
      "subscribers": [
        "common",
        "environment_STG"
      ],
      "interval": 10,
      "time_precision": "s",
      "handlers": [
        "metrics"
      ]
    }
  }
}
